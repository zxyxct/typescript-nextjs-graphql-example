    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "hire": "node -e \"const fs = require('fs'); const { execSync } = require('child_process'); const stdioOption = [0, 1, 2]; if (fs.existsSync('node_modules')) { execSync('npm run build', { stdio: stdioOption }); execSync('npm run start', { stdio: stdioOption }); } else { console.log('Installing node modules...'); execSync('npm install', { stdio: stdioOption }); execSync('npm run build', { stdio: stdioOption }); execSync('npm run start', { stdio: stdioOption }); }\""
  },
